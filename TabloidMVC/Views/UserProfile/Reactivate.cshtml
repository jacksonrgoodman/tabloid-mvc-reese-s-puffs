@model TabloidMVC.Models.UserProfile

@{
    ViewData["Title"] = "Reactivate";
}



<h4>Are you sure you want to reactivate @Model.FullName?</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Reactivate">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
            
                <input asp-for="Id" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="FirstName" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="LastName" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="DisplayName" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="Email" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="CreateDateTime" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="ImageLocation" type="hidden"/>
            </div>
            <div class="form-group">
                <input asp-for="UserTypeId"type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="Active" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="FullName" type="hidden" />
            </div>
            <div class="form-group">
                <input type="submit" value="Reactivate" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
